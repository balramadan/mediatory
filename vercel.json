{
  "framework": "nuxtjs",
  "buildCommand": "pnpm prisma generate && pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "false"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
